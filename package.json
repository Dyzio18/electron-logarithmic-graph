{
  "name": "electron-logarithmic-graph",
  "version": "1.0.0",
  "author": "Patryk Nizio",
  "license": "Apache License Version 2.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "chart.js"
  ],
  "build": {
    "appId": "chartApp",
    "category": "chartApp.education",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "chart.ico"
    }
  },
  "dependencies": {
    "electron": "^1.8.3",
    "electron-reload": "^1.2.2",
    "chart.js": "^2.7.2",
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "electron-builder": "^20.5.1"
  }
}
